{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "types": ["vitest/clients","@testing-library/jest-dom", "react-router-dom", "react", "react-dom"],
    "allowSyntheticDefaultImports": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}